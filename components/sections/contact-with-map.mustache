<section class="contact-with-map no-padding">
  <div class="container">
    <div class="row same-height-row">
      <div class="col-sm-12 col-lg-6 col-md-6 contact-container">
        <div class="section-heading align-left">
          <h3>Send us a message</h3>
          <p class="light-gray">Ask us anything on just sey hello!</p>
        </div>

        <form role="form" id="contact-form" class="contact-form">
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Name">
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <input type="email" class="form-control" name="email" autocomplete="off" id="email" placeholder="E-mail">
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <div class="form-group">
                <textarea class="form-control textarea" rows="3" name="Message" id="Message" placeholder="Message"></textarea>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <button type="submit" class="btn btn-rect btn-rect--default pull-right">Send a message</button>
            </div>
          </div>
        </form>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-12 map-column">
        <div class="map-container">
          <iframe
              style="width:100%;height:100%;border:0;"
              frameborder="0"
              src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA7pWbEO8HeWanfaLhCrwJwsjgxD4l0fWk
    &q=Space+Needle,Seattle+WA" allowfullscreen>
          </iframe>
        </div>
      </div>
    </div>
  </div>
</section>